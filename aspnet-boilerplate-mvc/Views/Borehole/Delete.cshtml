@model aspnet_boilerplate_mvc.Entities.Borehole
@{
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete Borehole</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>

    <div class="container">
        <h2>Delete Borehole</h2>

        <ol class="breadcrumb">
            <li><a href="/">Home</a></li>
            <li><a href="/Borehole/Index">Boreholes</a></li>
            <li class="active">Delete Borehole</li>
        </ol>

        <div class="alert alert-danger">
            <p>Are you sure you want to delete this borehole?</p>
        </div>

        <form id="deleteForm" action="/Borehole/DeleteConfirmed" method="post">
            <input name="__RequestVerificationToken" type="hidden" value="[anti-forgery token]" />
            <input type="hidden" name="Id" value="" />

            <div class="form-group">
                <label for="bhNumber">Bhnumber</label>
                <p id="bhNumber"></p>
            </div>

            <div class="form-group">
                <label for="gridReference">Gridreference</label>
                <p id="gridReference"></p>
            </div>

            <div class="form-group">
                <label for="latitude">Latitude</label>
                <p id="latitude"></p>
            </div>

            <div class="form-group">
                <label for="longitude">Longitude</label>
                <p id="longitude"></p>
            </div>

            <div class="form-group">
                <label for="depth">Depth</label>
                <p id="depth"></p>
            </div>

            <div class="form-group">
                <label for="sampling">Sampling</label>
                <p id="sampling"></p>
            </div>

            <div class="form-group">
                <label for="purposeId">PurposeId</label>
                <p id="purposeId"></p>
            </div>

            <div class="form-group">
                <label for="abstractionRate">AbstractionRate</label>
                <p id="abstractionRate"></p>
            </div>

            <div class="form-group">
                <label for="propertyInformationId">PropertyInformationId</label>
                <p id="propertyInformationId"></p>
            </div>

            <div class="form-group">
                <input type="submit" value="Delete" class="btn btn-danger" />
                <a href="/Borehole/Index" class="btn btn-default">Cancel</a>
            </div>
        </form>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>



</body>
</html>